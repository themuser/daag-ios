# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'daag' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for daag
  pod 'SwiftyJSON', '3.0.0'
  pod 'Alamofire', '~> 4.0'
  pod 'SDWebImage', '~>3.8'
  pod 'Toaster', '~> 2.0'
  pod 'Timepiece'
  pod 'Fabric'
  pod 'Crashlytics'


  target 'daagTests' do
  inherit! :search_paths
  # Pods for testing
  end

  target 'daagUITests' do
  inherit! :search_paths
  # Pods for testing
  end

end
